{
    "name": "alesito199/cna-project",
    "description": "Professional Upholstery Business Management System with Multi-language Support",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "firebase/php-jwt": "^6.8",
        "vlucas/phpdotenv": "^5.5",
        "mpdf/mpdf": "^8.2",
        "phpmailer/phpmailer": "^6.8",
        "aws/aws-sdk-php": "^3.275",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.21"
    },
    "autoload": {
        "psr-4": {
            "CNA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CNA\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}